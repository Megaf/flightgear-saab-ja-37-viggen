<?xml version="1.0"?>
<PropertyList>
    <model>
        <path>Aircraft/JA37/Models/Cockpit/switch-a/switch-a.xml</path>
        <name>switch-ignition-plug</name>
        <overlay>
            <params>
                <prop>ja37/engine/sparkplug</prop>
                <texture>SPARKPLUG.png</texture>
                <tooltip-text>Ignition plug</tooltip-text>
                <tooltip-id>switch-sparkplug</tooltip-id>
            </params>
        </overlay>
        <offsets>
            <x-m>  0.800</x-m>
            <y-m>  0.02</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>
    <model>
        <path>Aircraft/JA37/Models/Cockpit/switch-a/switch-a.xml</path>
        <name>switch-fuel-pneu</name>
        <overlay>
            <params>
                <prop>controls/fuel/pneumatic-flow</prop>
                <texture>PNEU-FUEL-FLOW.png</texture>
                <tooltip-text>Pneumatic fuel flow</tooltip-text>
                <tooltip-id>switch-fuel-pneu</tooltip-id>
            </params>
        </overlay>
        <offsets>
            <x-m>  0.625</x-m>
            <y-m>  0.02</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>
    <model>
        <path>Aircraft/JA37/Models/Cockpit/switch-a/switch-a.xml</path>
        <name>switch-fuel-elec</name>
        <overlay>
            <params>
                <prop>controls/fuel/electric-pump</prop>
                <texture>ELEC-FUELPUMP.png</texture>
                <tooltip-text>Electric fuel pump</tooltip-text>
                <tooltip-id>switch-fuel-elec</tooltip-id>
            </params>
        </overlay>
        <offsets>
            <x-m>  0.590</x-m>
            <y-m>  0.02</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>
    <model>
        <path>Aircraft/JA37/Models/Cockpit/switch-a/switch-a.xml</path>
        <name>switch-fuel-xtank</name>
        <overlay>
            <params>
                <prop>controls/fuel/xtank</prop>
                <texture>DROP-TANK.png</texture>
                <tooltip-text>Drop tank fuel flow</tooltip-text>
                <tooltip-id>switch-fuel-xtank</tooltip-id>
            </params>
        </overlay>
        <offsets>
            <x-m>  0.555</x-m>
            <y-m>  0.02</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>
    <model>
        <path>Aircraft/JA37/Models/Cockpit/switch-d/fuel-hydr-pump.xml</path>
        <name>switch-fuel-pneu</name>
        <offsets>
            <x-m>  0.510</x-m>
            <y-m>  0.02</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

    <model>
        <path>Aircraft/JA37/Models/Instruments/counter-measure-panel/cm.xml</path>
        <name>cm-p</name>
        <offsets>
            <x-m>  0.415</x-m>
            <y-m>  0.035</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

    <model>
        <path>Aircraft/JA37/Models/Cockpit/knob-a/weapons.xml</path>
        <name>weapon-select</name>
        <offsets>
            <heading-deg>-90</heading-deg>
            <x-m>  0.230</x-m>
            <y-m>  0.02625</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

    <animation>
        <type>select</type>
        <object-name>weapon-select</object-name>
        <condition>
           <greater-than>
            <property>ja37/systems/variant</property>
            <value>0</value>
           </greater-than>
        </condition>
    </animation>

    <model>
        <path>Aircraft/JA37/Models/Cockpit/knob-a/weapons-mode.xml</path>
        <name>weapon-mode</name>
        <offsets>
            <heading-deg>-90</heading-deg>
            <x-m>  0.230</x-m>
            <y-m>  0.02625</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

    <animation>
        <type>select</type>
        <object-name>weapon-mode</object-name>
        <condition>
           <equals>
            <property>ja37/systems/variant</property>
            <value>0</value>
           </equals>
        </condition>
    </animation>

    <model>
        <path>Aircraft/JA37/Models/Instruments/Datapanel/jett-tank.xml</path>
        <name>jett-xtank</name>
        <offsets>
            <heading-deg>-90</heading-deg>
            <x-m>  0.185</x-m>
            <y-m>  0.014</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

    <model>
        <path>Aircraft/JA37/Models/Instruments/Datapanel/jett-stores.xml</path>
        <name>jett-stores</name>
        <offsets>
            <heading-deg>-90</heading-deg>
            <x-m>  0.15</x-m>
            <y-m>  0.014</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

    <model>
        <path>Aircraft/JA37/Models/Cockpit/switch-d/sim.xml</path>
        <name>sim</name>
        <offsets>
            <heading-deg>0</heading-deg>
            <x-m>  0.105</x-m>
            <y-m>  0.011</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

    <model>
        <path>Aircraft/JA37/Models/Cockpit/switch-d/safety.xml</path>
        <name>safe</name>
        <offsets>
            <heading-deg>0</heading-deg>
            <x-m>  0.055</x-m>
            <y-m>  0.011</y-m>
            <z-m>  0</z-m>
        </offsets>
    </model>

</PropertyList>
