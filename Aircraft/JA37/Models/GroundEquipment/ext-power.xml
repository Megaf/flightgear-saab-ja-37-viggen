<?xml version="1.0"?>

<PropertyList>
 <path>ext-power.ac</path>
   
	<animation>
		<type>noshadow</type>
		<object-name>blah.012</object-name>
		<object-name>blah.010</object-name>
		<object-name>blah.003</object-name>
	</animation>
 
	<animation>
		<type>material</type>
		<object-name>blah.002</object-name> 
		<object-name>blah.004</object-name>
		<object-name>blah.005</object-name>
		<object-name>blah.006</object-name>
		<object-name>blah.007</object-name>
		<object-name>blah.008</object-name>
		<object-name>blah.009</object-name>
		<condition>
			<and>
				<not><property>/sim/rendering/rembrandt/enabled</property></not>
		      <greater-than>
		        <property>/sim/time/sun-angle-rad</property>
		        <value>1.22</value>
		      </greater-than>
		    </and>
		</condition>
		<emission>
		  <red>0.02</red>
		  <green>0.02</green>
		  <blue>0.02</blue>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>blah.010</object-name>
	    <condition>
	      <greater-than>
	        <property>/sim/time/sun-angle-rad</property>
	        <value>1.22</value>
	      </greater-than>
	    </condition>
		<emission>
		  <red>2.0</red>
		  <green>1.0</green>
		  <blue>0</blue>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>blah.003</object-name>
	    <condition>
	      <greater-than>
	        <property>/sim/time/sun-angle-rad</property>
	        <value>1.22</value>
	      </greater-than>
	    </condition>
		<emission>
		  <red>2.0</red>
		  <green>0.0</green>
		  <blue>0</blue>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>blah.012</object-name>
	    <condition>
	      <greater-than>
	        <property>/sim/time/sun-angle-rad</property>
	        <value>1.22</value>
	      </greater-than>
	    </condition>
		<emission>
		  <red>2.0</red>
		  <green>2.0</green>
		  <blue>2.0</blue>
		</emission>
	</animation>
</PropertyList>
