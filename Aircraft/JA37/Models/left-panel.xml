    <name>radar control</name>
    <offsets>
      <pitch-deg>-10</pitch-deg>
      <x-m>  0.550</x-m>
      <y-m>  0.05</y-m>
      <z-m>  0.0 </z-m>
    </offsets>
  </model>

  <animation>
    <type>translate</type>
    <object-name>radar control</object-name>
    <!-- Slight offset on the AJS -->
    <expression>
      <min>
        <property>ja37/systems/variant</property>
        <value>0.04</value>
      </min>
    </expression>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <model>
    <path>Aircraft/JA37/Models/Panels/Radio/fr31.xml</path>
    <name>fr31</name>
    <offsets>
