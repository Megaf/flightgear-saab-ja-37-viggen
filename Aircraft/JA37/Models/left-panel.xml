    <name>int-light</name>
    <offsets>
      <x-m>  0.860</x-m>
      <y-m>  0.065</y-m>
      <z-m>  0.00</z-m>
    </offsets>
  </model>

  <!-- JA only -->
  <animation>
    <type>select</type>
    <object-name>kv1</object-name>
    <object-name>kv3</object-name>
    <object-name>radar-panel-ja</object-name>
    <condition>
      <equals>
        <property>ja37/systems/variant</property>
        <value>0</value>
      </equals>
    </condition>
  </animation>

  <!-- AJS only -->
  <animation>
    <type>select</type>
    <object-name>radar-panel-ajs</object-name>
    <object-name>fr22</object-name>
    <condition>
      <greater-than>
        <property>ja37/systems/variant</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>
</PropertyList>
