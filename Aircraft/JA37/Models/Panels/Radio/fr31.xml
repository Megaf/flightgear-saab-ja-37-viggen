    <animation>
        <type>pick</type>
        <object-name>panel</object-name>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>rdio</tooltip-id>
                <label>FR29 Radio panel</label><!-- found info that seems to suggest it was called FR29 not FR31 -->
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>knobVolOuter</object-name>
        <property>ja37/sound/warnings-volume</property>
        <factor>600</factor><!-- 240 deg for 0-0.4 -->
        <offset-deg>-120</offset-deg>
        <center>
            <x-m>-0.04043</x-m>
            <y-m>0.04445</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>ja37/sound/warnings-volume</property>
                <factor>0.02</factor>
                <min>0</min>
                <max>0.40</max>
                <wrap>false</wrap>
           </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>warning-vol</tooltip-id>
                <label>Warnings volume</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>knobVolInner</object-name>
        <property>instrumentation/comm[0]/volume</property>
        <factor>240</factor>
        <offset-deg>-120</offset-deg>
        <center>
            <x-m>-0.04043</x-m>
            <y-m>0.04445</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/comm[0]/volume</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1.0</max>
                <wrap>false</wrap>
           </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>comm-vol</tooltip-id>
                <label>FR 29 comm volume</label>
            </binding>
        </hovered>
    </animation>

    <animation>
