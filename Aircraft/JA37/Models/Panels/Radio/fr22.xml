  <!-- Knobs -->
  <animation>
    <type>knob</type>
    <object-name>knob_group</object-name>
    <property>instrumentation/fr22/group</property>
    <factor>18</factor>
    <center>
      <x-m>0.002629</x-m>
      <y-m>-0.037588</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/fr22/group</property>
        <min>1</min>
        <max>41</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>knob-group</tooltip-id>
        <label>Group number</label>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>knob</type>
