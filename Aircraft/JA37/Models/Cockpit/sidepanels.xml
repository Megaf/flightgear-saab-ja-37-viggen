<?xml version="1.0"?>

<PropertyList>

    <path>sidepanels.ac</path>

  <effect>
    <inherits-from>Aircraft/JA37/Models/Effects/ja37-cockpit-shader</inherits-from>
    <!--<object-name>dashPanelMount</object-name>-->
    <object-name>innerWall_sides</object-name>
    <object-name>innerWall_front</object-name>
    <object-name>innerWall_back</object-name>
    <object-name>sidePanel_L</object-name>
    <object-name>sidePanel_R</object-name>
    <object-name>sidePanel_L_minor</object-name>
    <object-name>sidePanel_L_minor_forward</object-name>
    <object-name>sidePanel_R_minor_top</object-name>
    <object-name>sidePanel_R_minor_bottom</object-name>
    <object-name>sidePanel_R_mount_A</object-name>
    <object-name>sidePanel_R_mount_B</object-name>
    <object-name>sidePanel_R_mount_C</object-name>
    <object-name>sidePanel_L_mount_A</object-name>
    <object-name>sidePanel_L_mount_B</object-name>
    <object-name>dashPanelJA</object-name>
    <object-name>dashPanelAJ</object-name>
    <object-name>clockMountJA</object-name>
  </effect>

  <animation>
    <object-name>dashPanelJA</object-name>
    <object-name>clockMountJA</object-name>
    <type>select</type>
    <condition>
      <equals>
        <property>ja37/systems/variant</property>
        <value>0</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <object-name>dashPanelAJ</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>ja37/systems/variant</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

</PropertyList>
